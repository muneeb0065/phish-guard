{
    "manifest_version": 3,
    "name": "PhishGuard",
    "version": "1.0",
    "description": "React-based Phishing Detector",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Check Site Safety"
    },
    "icons": {
        "16": "vite.svg",
        "48": "vite.svg",
        "128": "vite.svg"
    }
}